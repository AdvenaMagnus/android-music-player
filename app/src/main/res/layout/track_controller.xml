                    android:layout_height="4dp"
                    android:layout_marginTop="@dimen/margin_eq"
                    android:paddingLeft="75dp"
                    android:paddingRight="@dimen/margin_eq"
                    android:progress="50"
                    android:progressDrawable="?attr/static_equilizer_stroke_style" />

                <ProgressBar
                    android:id="@+id/eq10"
